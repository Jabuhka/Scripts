-- –ö–Ω–æ–ø–∫–∞ –ü–µ—Ä–µ–∫–ª—é—á–∏—Ç—å –≤–∏–¥–∏–º–æ—Å—Ç—å –º–µ–Ω—é
local toggleButton = Instance.new("ImageButton")
toggleButton.Size = UDim2.new(0, 50, 0, 50)
toggleButton.Position = UDim2.new(0.9, -50, 0.1, 0)
toggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
toggleButton.Draggable = true
toggleButton.Image = "rbxassetid://17885941020"
toggleButton.Parent = screenGui

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è –≤–∏–¥–∏–º–æ—Å—Ç–∏ –º–µ–Ω—é
local menuVisible = false
toggleButton.MouseButton1Click:Connect(function()
	menuVisible = not menuVisible
	menuFrame.Visible = menuVisible
end)

CheeseButton.MouseButton1Click:Connect(function() 
becomeCheese()	
end)

PaperButton.MouseButton1Click:Connect(function() 
	scream()	
end)

SharButton.MouseButton1Click:Connect(function() 
	inflatePlayer()	
end)

UtkaButton.MouseButton1Click:Connect(function() 
	abductionEvent()	
end)

function inflatePlayer()
	for _, part in pairs(character:GetChildren()) do
		if part:IsA("BasePart") then
			part.Size = part.Size * 2
		end
	end
end


function scream()
	local sound = Instance.new("Sound", character.HumanoidRootPart)
	sound.SoundId = "rbxassetid://18925320031" -- –∫—Ä–∏—á–∞—â–∏–π –∑–≤—É–∫
	sound.Volume = 5
	sound:Play()
	game.Debris:AddItem(sound, 5)
end

function abductionEvent()
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")

	-- –°–æ–∑–¥–∞—ë–º UFO
	local ufo = Instance.new("Part")
	ufo.Size = Vector3.new(10, 1, 10)
	ufo.Shape = Enum.PartType.Cylinder
	ufo.Material = Enum.Material.Metal
	ufo.Color = Color3.fromRGB(150, 150, 150)
	ufo.Anchored = true
	ufo.CFrame = hrp.CFrame * CFrame.new(0, 50, 0)
	ufo.Parent = workspace

	-- –°–≤–µ—Ç–æ–≤–æ–π –ª—É—á
	local beam = Instance.new("Part")
	beam.Size = Vector3.new(5, 50, 5)
	beam.Transparency = 0.6
	beam.Anchored = true
	beam.CanCollide = false
	beam.Color = Color3.fromRGB(0, 255, 255)
	beam.Material = Enum.Material.Neon
	beam.CFrame = hrp.CFrame * CFrame.new(0, 25, 0)
	beam.Parent = workspace

	-- –≠—Ñ—Ñ–µ–∫—Ç –¥—ã–º–∞
	local smoke = Instance.new("Smoke", ufo)
	smoke.Color = Color3.new(0.5, 1, 0.5)
	smoke.Opacity = 0.3
	smoke.Size = 10

	-- –ó–≤—É–∫
	local sound = Instance.new("Sound", ufo)
	sound.SoundId = "rbxassetid://9126440642" -- –∏–Ω–æ–ø–ª–∞–Ω–µ—Ç–Ω—ã–π –∑–≤—É–∫
	sound.Volume = 1
	sound:Play()

	-- –ü–æ–¥–Ω–∏–º–∞–µ–º –∏–≥—Ä–æ–∫–∞
	local bp = Instance.new("BodyPosition", hrp)
	bp.MaxForce = Vector3.new(100000, 100000, 100000)
	bp.Position = ufo.Position + Vector3.new(0, -5, 0)

	-- –£–ª–µ—Ç–∞–µ—Ç —á–µ—Ä–µ–∑ 5 —Å–µ–∫
	task.delay(5, function()
		ufo:Destroy()
		beam:Destroy()
		smoke:Destroy()
		bp:Destroy()
	end)
end




-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è –∫–Ω–æ–ø–∫–∏
local dragging = false
local dragInput, dragStart, startPos

toggleButton.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = toggleButton.Position
	end
end)

toggleButton.InputChanged:Connect(function(input)
	if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
		local delta = input.Position - dragStart
		toggleButton.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
end)

toggleButton.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = false
	end
end)


local spinningZ = false
local speed = 0
local maxSpeed = 30
local acceleration = 0.5
local exploded = false

spinButton.MouseButton1Click:Connect(function()
	if not spinningZ then
		spinningZ = true
		speed = 0
		exploded = false
		spinButton.Text = "–•–≤–∞—Ç–∏—Ç!"
	else
		spinningZ = false
		spinButton.Text = "–ó–∞–∫—Ä—É–∂–∏—Ç—å!"
	end
end)

local function explodePlayer()
	if character and character:FindFirstChild("HumanoidRootPart") then
		local explosion = Instance.new("Explosion")
		explosion.Position = character.HumanoidRootPart.Position
		explosion.BlastRadius = 10
		explosion.BlastPressure = 500000
		explosion.Parent = workspace

		for _, part in pairs(character:GetChildren()) do
			if part:IsA("BasePart") then
				part.Anchored = false
				part.Velocity = Vector3.new(
					math.random(-100, 100),
					math.random(50, 150),
					math.random(-100, 100)
				)
			end
		end

		if character:FindFirstChild("Humanoid") then
			character.Humanoid.Health = 0
		end
	end
end

-- ‚òÅÔ∏è ANTI-GRAVITY GUI

local flyingUp = false
local floatForce = nil

gravityButton.MouseButton1Click:Connect(function()
	if flyingUp then return end
	flyingUp = true
	gravityButton.Text = "–õ–µ—Ç–∏–º... ‚òÅÔ∏è"

	local root = character:WaitForChild("HumanoidRootPart")
	floatForce = Instance.new("BodyVelocity")
	floatForce.Velocity = Vector3.new(0, 50, 0)
	floatForce.MaxForce = Vector3.new(0, 5000, 0)
	floatForce.P = 1250
	floatForce.Parent = root

	task.delay(5, function()
		if floatForce then
			floatForce:Destroy()
			floatForce = nil
		end
		gravityButton.Text = "–ë–ê–•! üí•"
		wait(1.5)
		gravityButton.Text = "–ê–Ω—Ç–∏–≥—Ä–∞–≤–∏—Ç–∞—Ü–∏—è"
		Instance.new("Explosion", character)
		flyingUp = false
	end)
end)

-- üöÄ FLY GUI

local flying = false
local flySpeed = 50
local bodyGyro
local bodyVelocity

local function startFlying()
	local root = character:WaitForChild("HumanoidRootPart")

	bodyGyro = Instance.new("BodyGyro")
	bodyGyro.MaxTorque = Vector3.new(400000, 400000, 400000)
	bodyGyro.P = 100000
	bodyGyro.CFrame = root.CFrame
	bodyGyro.Parent = root

	bodyVelocity = Instance.new("BodyVelocity")
	bodyVelocity.Velocity = Vector3.new(0, 0, 0)
	bodyVelocity.MaxForce = Vector3.new(400000, 400000, 400000)
	bodyVelocity.P = 1250
	bodyVelocity.Parent = root

	flying = true
	flyButton.Text = "üõë Unfly"
end

local function stopFlying()
	if bodyGyro then bodyGyro:Destroy() end
	if bodyVelocity then bodyVelocity:Destroy() end
	flying = false
	flyButton.Text = "üöÄ Fly"
end

flyButton.MouseButton1Click:Connect(function()
	if flying then
		stopFlying()
	else
		startFlying()
	end
end)

-- –ö–Ω–æ–ø–∫–∞ F (–ü–ö)
UserInputService.InputBegan:Connect(function(input, processed)
	if processed then return end
	if input.KeyCode == Enum.KeyCode.F then
		if flying then
			stopFlying()
		else
			startFlying()
		end
	end
end)

local Camera = workspace.CurrentCamera

game:GetService("RunService").RenderStepped:Connect(function(deltaTime)
	if character and character:FindFirstChild("HumanoidRootPart") then
		if spinningZ or speed > 0 then
			-- –ª—ë–≥–∫–∏–π –Ω–∞–∫–ª–æ–Ω —ç–∫—Ä–∞–Ω–∞ —Ç—É–¥–∞-—Å—é–¥–∞
			local shakeAngle = math.sin(tick() * 10) * 0.5
			Camera.CFrame *= CFrame.Angles(0, 0, shakeAngle)
		end
	end
end)

function becomeCheese()
	for _, part in pairs(character:GetDescendants()) do
		if part:IsA("BasePart") then
			part.Material = Enum.Material.Foil
			part.Color = Color3.fromRGB(255, 255, 102) -- –∂–µ–ª—Ç—ã–π —Å—ã—Ä
		end
	end
end

-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–π –∫–∞–¥—Ä
game:GetService("RunService").RenderStepped:Connect(function()
	character = player.Character or player.CharacterAdded:Wait()
	if character:FindFirstChild("HumanoidRootPart") then
		local root = character.HumanoidRootPart

		-- –ö—Ä—É—á–µ–Ω–∏–µ
		if spinningZ then
			if speed < maxSpeed then
				speed += acceleration
			else
				if not exploded then
					exploded = true
					spinningZ = false
					spinButton.Text = "üí•"
					explodePlayer()
				end
			end
			root.CFrame *= CFrame.Angles(0, math.rad(speed), 0)
		elseif speed > 0 then
			speed -= acceleration * 2
			if speed < 0 then speed = 0 end
			root.CFrame *= CFrame.Angles(0, math.rad(speed), 0)
		end

		-- –ü–æ–ª—ë—Ç
		if flying and bodyGyro and bodyVelocity then
			bodyGyro.CFrame = camera.CFrame
			bodyVelocity.Velocity = camera.CFrame.LookVector * flySpeed
		end
	end
end)

--–í–µ—Ä—Ç–æ–ª–µ—Ç
local spinning = false

local function startVertolet()
	local root = character:WaitForChild("HumanoidRootPart")
	local spin = Instance.new("BodyAngularVelocity")
	spin.AngularVelocity = Vector3.new(0, 10, 0)
	spin.MaxTorque = Vector3.new(0, math.huge, 0)
	spin.P = 1250
	spin.Parent = root
	spinning = true
	return spin
end

local function stopVertolet(spin)
	if spin then spin:Destroy() end
	spinning = false
end

local activeSpin = nil
spinButtonV.MouseButton1Click:Connect(function()
	if spinning then
		stopVertolet(activeSpin)
		spinButtonV.Text = "üåÄ –í–µ—Ä—Ç–æ–ª–µ—Ç"
	else
		activeSpin = startVertolet()
		spinButtonV.Text = "üõë –û—Å—Ç–∞–Ω–æ–≤–∏—Ç—å"
	end
end)

--–†–µ–∂–∏–º –ñ–∞–±–∫–∏
local croaking = false

local function startCroaking()
	local humanoid = character:FindFirstChild("Humanoid")
	if not humanoid then return end

	croaking = true
	while croaking do
		humanoid.Jump = true

		local sound = Instance.new("Sound", character:WaitForChild("Head"))
		sound.SoundId = "rbxassetid://12221967" -- –∫–≤–∞–∫–∞—é—â–∞—è –∂–∞–±–∞
		sound.Volume = 1
		sound:Play()
		game.Debris:AddItem(sound, 2)

		task.wait(1.5)
	end
end

local function stopCroaking()
	croaking = false
end

frogButton.MouseButton1Click:Connect(function()
	if croaking then
		stopCroaking()
	else
		startCroaking()
	end
end)
